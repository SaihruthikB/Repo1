package arraysClass;

public class Java_Arrays {
	 
	public static void main(String[] args) {
		
            int a[] = {10,2,5,20,1,100};
            int n=a.length;

            for(int i=0; i<n-1;i++)
            {
            	for(int j=i+1;j<n;j++)
            	{
            		if(a[j]<a[i])
            		{
            			a[i]=j;
            		}
            	}
            }

		 
	}
	}


