package arraysClass;

public class DuplicateNumberInArray {

	public static void main(String[] args) {

		int a[] = { 10, 20, 30, 30, 40, 50 };
		int n =a.length;
		int target = 60;
		for(int i=0;i<n;i++)
		{
			for(int j=i+1;j<n;j++)
			{
				if(a[i]+a[j]==60)
				{
					System.out.println("The Pair whose sum is equal to 60 is"+a[i] + "" + "" +a[j]);
				}
			}
		}

	}

}
